{
  "name": "screen-2-view",
  "version": "1.1.0",
  "description": "Aplicativo para reprodução de imagens em segunda tela.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "build": {
    "appId": "com.github.joaohcrangel.electron-screen-2-view",
    "productName": "electron-screen-2-view",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "joaohcrangel",
        "repo": "electron-screen-2-view"
      }
    ]
  },
  "repository": "https://github.com/joaohcrangel/electron-screen-2-view.git",
  "author": "João Rangel",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.4",
    "jquery": "^3.5.0",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^1.0.0-rc.2",
    "nedb": "^1.8.0",
    "popper.js": "^1.14.5"
  }
}
